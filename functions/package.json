    "firebase-functions": "^5.0.0",
    "plaid": "^28.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "firebase-functions-test": "^3.3.0",
    "eslint": "^8.57.0"
  },
  "private": true
}
